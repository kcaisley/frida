library (sampswitch) {
  capacitive_load_unit (1,pf);
  current_unit : "1mA";
  delay_model : "table_lookup";
  nom_process : 1;
  nom_temperature : 25;
  nom_voltage : 1.2;
  pulling_resistance_unit : "1kohm";
  simulation : true;
  time_unit : "1ns";
  voltage_unit : "1V";
  default_fanout_load : 1;
  
  type (bus1) {
    base_type : "array";
    data_type : "bit";
  }

  cell (samp_p) {
    area : 1;
    
    pin (clk) {
      capacitance : 0.005;
      direction : "input";
    }
    
    pin (vin) {
      capacitance : 0.005;
      direction : "input";
    }
    
    pin (dout) {
      direction : "output";
    }
  }
}
